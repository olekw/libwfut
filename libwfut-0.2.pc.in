Requires: @LIBWFUT_REQUIRES@
Version: @VERSION@
Libs: -L${libdir} -lwfut@LIBWFUT_LIB_SUFFIX@
Cflags: -I${includedir}/libwfut@LIBWFUT_LIB_SUFFIX@
